{
  "name": "riadvice/bbbeasy-backend",
  "description": "BBBEasy - Web-Application Backend",
  "type": "project",
  "autoload": {
    "psr-4": {
      "": "app/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": [
        "app/src",
        "tests/src/"
      ]
    }
  },
  "require": {
    "ext-json": "*",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-bcmath": "*",
    "ext-fileinfo": "*",
    "bcosca/fatfree-core": "^3.8.2",
    "ikkez/f3-cortex": "^1.7.6",
    "ikkez/f3-mailer": "dev-master#972d715 as 1.2.3",
    "respect/validation": "^2.2.4",
    "nette/utils": "^v4.0.0",
    "tracy/tracy": "^v2.10.2",
    "monolog/monolog": "^3.3.1",
    "bigbluebutton/bigbluebutton-api-php": "dev-master",
    "robmorgan/phinx": "^0.13.4",
    "symfony/yaml": "^v6.2.10",
    "linfo/linfo": "^v4.0.7",
    "roave/better-reflection": "^6.10.0",
    "haydenpierce/class-finder": "^0.5.2",
    "marc-mabe/php-enum-phpstan": "^v2.0.1",
    "yohang/finite": "^1.3.5",
    "marc-mabe/php-enum": "^v4.7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "ext-xdebug": ">=3.2.0",
    "kint-php/kint": "^5.0.5",
    "fakerphp/faker": "^v1.23.0",
    "friendsofphp/php-cs-fixer": "^v3.15.1",
    "phpunit/php-code-coverage": "^10.1.2",
    "kevinlebrun/colors.php": "^1.0.3",
    "gabrielelana/byte-units": "^0.5.0",
    "phpmetrics/phpmetrics": "^v2.8.2",
    "devster/ubench": "^2.1.0",
    "rector/rector": "^0.15.25"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": false,
    "process-timeout": 600,
    "preferred-install": "dist",
    "github-protocols": [
      "https"
    ]
  },
  "scripts": {"csfix": [" sudo phpdismod -s cli xdebug",
      "vendor/bin/php-cs-fixer fix",
      "sudo phpenmod -s cli xdebug\n\n"
      ]
  },
  "suggest": {
    "xfra35/f3-cron": "Job scheduling for the PHP Fat-Free Framework"
  },
  "license": "PRIVATE",
  "authors": [
    {
      "name": "RIADVICE",
      "email": "devops@riadvice.tn"
    }
  ]
}
