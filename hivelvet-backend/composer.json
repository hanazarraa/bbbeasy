{
  "name": "riadvice/hivelvet-backend",
  "description": "Hivelvet - Web-Application Backend",
  "type": "project",
  "autoload": {
    "psr-4": {
      "": "app/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": [
        "app/src",
        "tests/src/"
      ]
    }
  },
  "require": {
    "ext-json": "*",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-bcmath": "*",
    "ext-fileinfo": "*",
    "bcosca/fatfree-core": "3.8.1",
    "ikkez/f3-cortex": "dev-master#65213c4 as 1.7.5",
    "ikkez/f3-mailer": "^1.2",
    "respect/validation": "^2.2",
    "nette/utils": "^3.1",
    "tracy/tracy": "^2.7",
    "monolog/monolog": "^2.0",
    "bigbluebutton/bigbluebutton-api-php": "dev-master",
    "robmorgan/phinx": "^0.12",
    "symfony/yaml": "^6.0",
    "linfo/linfo": "^4.0",
    "carbon-cli/carbon-cli": "^1.2",
    "roave/better-reflection": "^5.0",
    "haydenpierce/class-finder": "^0.4.3",
    "marc-mabe/php-enum-phpstan": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "ext-xdebug": ">=2.3.3",
    "kint-php/kint": "^4.0",
    "fakerphp/faker": "^1.13",
    "friendsofphp/php-cs-fixer": "^3.6",
    "phpunit/php-code-coverage": "^9.2.0",
    "kevinlebrun/colors.php": "^1.0",
    "gabrielelana/byte-units": "^0.5",
    "phpmetrics/phpmetrics": "^2.7",
    "devster/ubench": "^2.1",
    "rector/rector": "^0.12.16",
    "marc-mabe/php-enum": "^4.7"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": false,
    "process-timeout": 600,
    "preferred-install": "dist",
    "github-protocols": [
      "https"
    ]
  },
  "scripts": {"csfix": [" sudo phpdismod -s cli xdebug",
      "vendor/bin/php-cs-fixer fix",
      "sudo phpenmod -s cli xdebug\n\n"
      ]
  },
  "suggest": {
    "xfra35/f3-cron": "Job scheduling for the PHP Fat-Free Framework"
  },
  "license": "PRIVATE",
  "authors": [
    {
      "name": "RIADVICE",
      "email": "devops@riadvice.tn"
    }
  ]
}
